  "scripts": {
    "start": "tsoa routes && tsoa swagger && ts-node build.ts && tsc && node dist/index.js",
    "debug": "tsoa routes && tsoa swagger && nodemon --inspect -r ts-node/register server/index.ts",
    "compile": "tsoa routes && tsoa swagger && ts-node build.ts && tsc",
    "dev": "tsoa routes && tsoa swagger && nodemon server/index.ts",
    "dev:debug": "tsoa routes && tsoa swagger && nodemon --exec node -r ts-node/register --inspect-brk server/index.ts",
    "lint": "tsc --noEmit && eslint -c .eslintrc.js server/**/*.ts --quiet",
    "lint:fix": "tsc --noEmit && eslint -c .eslintrc.js server/**/*.ts --quiet --fix",
    "test": "mocha test/**/*.ts --exit",
    "test:debug": "mocha --inspect-brk test/**/*.ts --exit",
    "generate:swagger": "tsoa routes && tsoa swagger"
  },
